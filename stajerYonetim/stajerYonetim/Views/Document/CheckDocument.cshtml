
@{
    ViewData["Title"] = "CheckDocument";
    Layout = "~/Views/Shared/Admin_Layout.cshtml";
}

<br />
<table class="table table-bordered" style="color:white">
    <tr>
        <td>Project Name</td>
        <td>Project Managers Id's</td>
        <td>Download</td>
    </tr>
    @foreach (var x in Model)
    {
        <tr>
            <td>@x.OriginalFileName</td>
            <td>@x.InternId</td>
            <td><a href="/Document/Download/@x.Id" class="btn btn-primary">Download</a></td>

        </tr>
    }
</table>


